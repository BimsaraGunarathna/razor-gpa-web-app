@page
@model razor_gpa_web_app.Pages.Years.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Year[0].YearNumber)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Year) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.YearNumber)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.YearID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.YearID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.YearID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
